package javaBasic2.ch05.sec02;

public class 참조타입2 {

	public static void main(String[] args) {
		
		// A클래스를 힙메모리에 올리기 참조변수 aAddr
		A aAddr = new A();

		// 홍길동 입력
		aAddr.name = "홍길동";
		aAddr.kor = 100;
		aAddr.avg = 3.5;
		
		//교재 147쪽
		A aAddr2 = new A();
		
		System.out.println( aAddr == aAddr2);
	}

}
