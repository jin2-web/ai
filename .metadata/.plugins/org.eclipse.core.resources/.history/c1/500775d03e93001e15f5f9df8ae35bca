package javaBasic.ch04.sec01;

public class While문 {

	public static void main(String[] args) {
		// 반복문
		// 형식 while(조건) { 참이면 수행할 문장 }
		// 1~100 출력하기
		// 숫자변수 만들어 1출력하기 -> 변수 + 1 -> 출력 
		// 1 2
		int number=1;
		System.out.println( number );
		number++;
		System.out.println( number );
		

	}

}
